generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model compromisso {
  id         Int      @id @default(autoincrement())
  start_datetime      DateTime @db.Timestamptz(3)
  end_datetime        DateTime @db.Timestamptz(3)
  description         String

  @@map("compromissos") // nome da tabela no Postgres
}